# Cine Q
q.test {

    @ws {
        header Connection *Upgrade*
        header Upgrade    websocket
    }
    reverse_proxy @ws php-worker:6001

    root * /var/www/q/public/
    php_fastcgi php-fpm:9000
    file_server

    encode gzip
}

